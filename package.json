{
  "name": "cloudflare-wallet",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "pages:dev": "wrangler pages dev dist",
    "pages:deploy": "npm run build && wrangler pages deploy dist",
    "deploy": "bash scripts/deploy.sh",
    "init": "bash scripts/init-instance.sh",
    "db:create": "wrangler d1 create",
    "db:migrate": "wrangler d1 execute --remote --file=schema.sql",
    "db:seed": "wrangler d1 execute --remote --file=scripts/seed-data.sql",
    "seed:superadmin": "node scripts/seed-superadmin.js"
  },
  "dependencies": {
    "@noble/curves": "2.0.1",
    "@noble/hashes": "2.0.1",
    "@noble/secp256k1": "3.0.0",
    "@scure/base": "2.0.0",
    "@simplewebauthn/browser": "13.2.2",
    "@simplewebauthn/server": "13.2.2",
    "@web3icons/react": "4.1.17",
    "bs58": "6.0.0",
    "cryptocurrency-icons": "0.18.1",
    "html5-qrcode": "2.3.8",
    "jsonwebtoken": "9.0.3",
    "lucide-react": "0.562.0",
    "qrcode.react": "4.2.0",
    "react": "19.2.4",
    "react-dom": "19.2.4"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "4.20260203.0",
    "@types/jsonwebtoken": "9.0.10",
    "@types/node": "24.10.10",
    "@types/qrcode.react": "1.0.5",
    "@types/react": "19.2.10",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-react": "5.1.3",
    "typescript": "5.9.3",
    "vite": "7.3.1",
    "wrangler": "4.62.0"
  }
}
